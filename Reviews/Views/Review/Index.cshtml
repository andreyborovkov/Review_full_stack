@model IEnumerable<Reviews.Models.Review>
@{
    ViewData["Title"] = "Reviews";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <h1>All Reviews</h1>
    @foreach (var review in Model)
    {
        <div>
            <a href="/Review/Details/@review.Id">
                @review.Title
            </a>
        </div>
    }
