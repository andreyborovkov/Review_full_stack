
@model IEnumerable<Reviews.Models.Category>

  @{
      ViewData["Title"] = "Categories";
      Layout = "~/Views/Shared/_Layout.cshtml";
  }

    <h1>All Categories</h1>
    @foreach (var category in Model)
    {
        <div>
            <a href="/Category/Details/@category.Id">
                @category.Name
            </a>
        </div>
    }











